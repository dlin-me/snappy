{
    "name": "dlin/snappy-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for Snappy ( wkhtmltopdf )",
    "keywords": ["pdf", "symfony", "wkhtmltopdf"],
    "license": "MIT",
    "version": "0.9.1",
    "homepage": "http://dlin.me",

    "authors": [
        {
            "name": "David Lin",
            "email": "davidforest@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.3.8",
        "knplabs/knp-snappy": "*",
        "google/wkhtmltopdfAamd64": "0.11.0",
        "google/wkhtmltopdfI386": "0.11.0"

    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.0",
        "symfony/finder": "2.3.*"
    },

    "autoload": {
        "psr-0": { "Dlin\\Bundle\\SnappyBundle": "" }
    },

    "target-dir": "Dlin/Bundle/SnappyBundle",

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "google/wkhtmltopdfAamd64",
                "version": "0.11.0",
                "dist": {
                    "url": "http://wkhtmltopdf.googlecode.com/files/wkhtmltopdf-0.11.0_rc1-static-amd64.tar.bz2",
                    "type": "tar"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/wkhtmltopdfI386",
                "version": "0.11.0",
                "dist": {
                    "url": "http://wkhtmltopdf.googlecode.com/files/wkhtmltopdf-0.11.0_rc1-static-i386.tar.bz2",
                    "type": "tar"
                }
            }
        }
    ]
}
